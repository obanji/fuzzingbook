     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "my_istack = InputStack('hello world')\n",
    "my_istack.ignored(1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1220,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "True"
      ]
     },
     "execution_count": 1220,
     "metadata": {},
     "output_type": "execute_result"
    }
